x = "hey"
print(x)
# forloops
# forloops
fruits = ["apple", "orange", "cherry"]
for x in fruits:
    print(x)
for x in fruits:
    print(x)
# looping through a string
fruits = ["apple", "orange", "cherry"]
for x in " banana":
    print(x)
for y in "kajirwal":
    print(y)
# hujik
fruits = ['apple', 'orange', 'grapes']
for x in fruits:
    if x == 'banana':
        continue
    print(x)
    # range
for x in range(6):
    print(x)
for x in range(2, 5):
    print(x)
for x in range(2, 10, 2):
    print(x)
# else in for loop
for x in range(4):
    print(x)
# default parameter value


def my_function(country="norway"):
    print("iam  from" + country)


my_function(" sweden")
my_function(" netherland")
my_function(" canada")
my_function(" imdia")
